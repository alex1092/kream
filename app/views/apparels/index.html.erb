<p id="notice"><%= notice %></p>


<h1>Apparel</h1>

<table>
  <thead>
    <tr>
      <th>Brand</th>
      <th>Model</th>
      <th>Size</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @apparels.each do |apparel| %>
      <tr>
        <td><%= apparel.brand %></td>
        <td><%= apparel.model %></td>
        <td><%= apparel.size %></td>
        <td><%= apparel.price %></td>
        <td><%= link_to 'Show', apparel %></td>
        <td><%= link_to 'Edit', edit_apparel_path(apparel) %></td>
        <td><%= link_to 'Destroy', apparel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


